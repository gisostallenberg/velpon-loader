{
    "name": "gisostallenberg/velpon-loader",
    "description": "Dynamic glue for classes in a stack",
    "type": "library",
    "authors": [
        {
            "name": "Giso Stallenberg",
            "email": "gisostallenberg@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "GisoStallenberg\\VelponLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Resources/test_classes/"
        ],
        "psr-4": {
            "GisoStallenberg\\VelponLoader\\Tests\\": "tests/"
        }
    }
}
